<?xml version="1.0" encoding="utf-8"?>

<Defs>

  <!-- 燒結爐 -->

  <RecipeDef>
    <defName>DMS_Make_Tungsteel</defName>
    <label>Make tungsteel X20</label>
    <description>Sinter a small amount of tungsteel.</description>
    <jobString>Sintering.</jobString>
    <workSpeedStat>SmeltingSpeed</workSpeedStat>
    <effectWorking>ConstructMetal</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <!-- <unfinishedThingDef>UnfinishedComponent</unfinishedThingDef>-->
    <recipeUsers>
      <li>DMS_Furnace</li>
      <li>EAF</li>
    </recipeUsers>
    <skillRequirements>
      <Crafting>12</Crafting>
    </skillRequirements>
    <workSkill>Crafting</workSkill>
    <researchPrerequisite>DMS_Material</researchPrerequisite>
    <workAmount>4000</workAmount>
    <ingredients>
      <li>
        <filter>
          <thingDefs>
            <li>Wolframite</li>
            <li>Tungsten</li>
          </thingDefs>
        </filter>
        <count>10</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>Silicon</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
      <li>
        <filter>
          <thingDefs>
            <li>AluminiumBar</li>
          </thingDefs>
        </filter>
        <count>5</count>
      </li>
    </ingredients>
    <fixedIngredientFilter>
      <thingDefs>
        <li>Wolframite</li>
        <li>Tungsten</li>
      </thingDefs>
    </fixedIngredientFilter>
    <products>
      <Tungsteel>20</Tungsteel>
    </products>
  </RecipeDef>

